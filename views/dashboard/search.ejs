<div class="container-fluid container-fluid-custom pb-5 mb-5">
    <div class="row">
        <% if( searchResults !='' ) { %>
            <% searchResults.forEach(function (note,index) { %>
                <a href="/dashboard/item/<%= note._id %>">
                  <h4><%= note.title %></h4>  
                </a>
            <% }) %>    
        <% } else { %>
            <P>No item found...</P>
        <% } %>    
    </div>


</div>